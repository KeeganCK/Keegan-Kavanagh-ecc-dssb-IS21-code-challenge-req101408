{
    "$schema": "https://json-schema.org/draft-07/schema",
    "$id": "https://example.com/product.schema.json",
    "title": "IMB Project Schema",
    "description": "A project for IMB",
    "type": "object",
    "properties": {
        "productId": {
            "type": "string"
        },
        "productName": {
            "type": "string"
        },
        "productOwnerName": {
            "type": "string"
        },
        "Developers": {
            "type": "array",
            "items": {
                "type": "string"
            },
            "minItems": 1,
            "maxItems": 5,
            "uniqueItems": true
        },
        "scrumMasterName": {
            "type": "string"
        },
        "startDate": {
            "type": "string"
        },
        "methodology": {
            "type": "string"
        },
        "location": {
            "type": "string"
        }
    },
    "required": [ "productId", "productName", "productOwnerName", "Developers", "scrumMasterName", "startDate", "methodology", "location" ]
}

